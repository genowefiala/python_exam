def process_analyzer():

        print "Parsing process-list.txt in current directory ... done."
        print "Analysing process list ... done."

        fname = "./process-list.txt"
        user_processes = [] 

        with open(fname) as f:
                next(f)
                for i, line in enumerate(f):
                        if "     \_ " in line:
                                user_processes.append(line)

        print "Total number of process is:",  i + 1
        print "Number of user-invoked processes", len(user_processes)  

process_analyzer()


